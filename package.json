{
  "name": "nestjs-pg-pubsub",
  "version": "0.1.0",
  "description": "NestJS Postgres pubsub module",
  "author": "Jacob Correa",
  "license": "MIT",
  "url": "https://github.com/jkcorrea/nestjs-pg-pubsub#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepare": "npm run build",
    "test": "jest --runInBand",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/platform-express": "7.1.3",
    "@nestjs/testing": "7.1.3",
    "@types/jest": "25.2.3",
    "@types/node": "7.10.8",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "prettier": "2.0.5",
    "release-it": "13.6.2",
    "rimraf": "3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@nestjs/common": "7.1.3",
    "@nestjs/core": "7.1.3",
    "@types/pg": "7.14.3",
    "pg": "8.2.1",
    "pg-listen": "1.5.1",
    "reflect-metadata": "0.1.13"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7.0.0",
    "@nestjs/core": ">=7.0.0",
    "pg": ">=8.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "keywords": [
    "nestjs",
    "nest",
    "pg",
    "pubsub",
    "pg-pubsub"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkcorrea/nestjs-pg-pubsub"
  }
}
